
pipeline {
  
    agent any

    stages {
      
        stage("build") {
            steps {
              echo 'Pipeline building!'
            }
        }
        stage("test") {
            steps {
              echo 'Pipeline testing!'
            }
        }
        stage("deploy") {
            steps {
              echo 'Pipeline deploying success'
            }
        }
    }   
}
